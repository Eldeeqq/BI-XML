                        </fo:block>
                        <fo:block text-align="left" margin-bottom="0.25cm">
                           <fo:block font-weight="700">subscriptions per 100 inhabitants</fo:block>
                           <fo:block>7</fo:block>
                        </fo:block>
                     </fo:block>
                  </fo:block>
                  <fo:block text-align="left">
                     <fo:block font-weight="700">Telephones - mobile cellular</fo:block>
                     <fo:block>
                        <fo:block text-align="left" margin-bottom="0.25cm">
                           <fo:block font-weight="700">total</fo:block>
                           <fo:block>76,653,421</fo:block>
                        </fo:block>
